CREATE TABLE Profile(
    id TEXT PRIMARY KEY NOT NULL ,
    username TEXT NOT NULL ,
    email TEXT NOT NULL ,
    userRole TEXT NOT NULL ,
    name TEXT NOT NULL ,
    phone TEXT NOT NULL ,
    studentRoleType TEXT NOT NULL ,
    lastName TEXT NOT NULL
);


getUserProfile:
    SELECT * FROM Profile;



setProfile:
    INSERT OR REPLACE INTO Profile(id, username, email, userRole, name, phone, studentRoleType, lastName)
    VALUES (?,?,?,?,?,?,?,?);